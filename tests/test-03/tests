ff --case ignore case
lowercase
UpperCase



ff --case smart case
lowercase
UpperCase



ff --case sensitive case
lowercase



ff --case ignore Case
lowercase
UpperCase



ff --case smart Case
UpperCase



ff --case sensitive Case
UpperCase



ff lower
lowercase



ff --case ignore lower
lowercase



ff --case smart lower
lowercase



ff --case sensitive lower
lowercase



ff --case ignore upper
UpperCase



ff --case smart upper
UpperCase



ff --case sensitive upper



ff --case ignore Upper
UpperCase



ff --case smart Upper
UpperCase



ff --case sensitive Upper
UpperCase



ff --case sensitive name%Up*
UpperCase



ff --case sensitive name%up*
