ff -S
image.jpg
image.png
video.mp4



ff -S size
image.jpg
image.png
video.mp4



# no_order
ff mime.type=image
image.jpg
image.png



ff mime.type=video
video.mp4



ff --no-cache height+=1080
video.mp4



ff --no-cache height-=1080
image.jpg
image.png
video.mp4



ff --no-cache duration+=5s
video.mp4



# keep_order
ff -o mime -S -R
video/mp4
image/png
image/jpeg



# Check references.
# no_order
ff mime.type={}image.jpg
image.jpg
image.png



ff mime.type={mime.type}image.jpg
image.jpg
image.png



ff height+={width}image.png
video.mp4
